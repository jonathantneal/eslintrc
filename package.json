{
	"name": "eslint-config-dev",
	"version": "3.2.0",
	"description": "A shareable configuration package for eslint",
	"author": "Jonathan Neal <jonathantneal@hotmail.com>",
	"license": "CC0-1.0",
	"repository": "jsxtools/eslint-config-dev",
	"homepage": "https://github.com/jsxtools/eslint-config-dev#readme",
	"bugs": "https://github.com/jsxtools/eslint-config-dev/issues",
	"main": ".eslintrc",
	"files": [
		".eslintrc",
		"config"
	],
	"exports": {
		".": "./config/js.json",
		"./package.json": "./package.json",
		"./astro": "./config/astro.json",
		"./astro-strict": "./config/astro-strict.json",
		"./js": "./config/js.json",
		"./js-strict": "./config/js-strict.json",
		"./ts": "./config/ts.json",
		"./ts-strict": "./config/ts-strict.json"
	},
	"scripts": {
		"prepublish": "npm test",
		"release": "npm publish --access public",
		"test": "node test.mjs"
	},
	"devDependencies": {
		"@typescript-eslint/parser": "^5",
		"@typescript-eslint/eslint-plugin": "^5",
		"eslint": "^8.36",
		"eslint-plugin-astro": "^0.24",
		"eslint-plugin-jsx-a11y": "^6"
	},
	"peerDependencies": {
		"eslint": "^8"
	},
	"keywords": [
		"eslint",
		"eslint-config",
		"dev"
	]
}
